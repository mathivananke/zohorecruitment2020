#include <stdio.h>

int main()
{
    int m, i, j;

   
    printf("Enter number of input: ");
    scanf("%d", &m);
   

    for(i=1; i<=m; i++)
    {
        for(j=m; j>m-i; j--)
        {
            printf("%d", j);
        }

        for(j=1; j<=m-i; j++)
        {
            printf("%d", (m - i + 1));
        }

        printf("\n");
    }

    return 0;
}
